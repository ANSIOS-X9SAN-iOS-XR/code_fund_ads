<% content_for :breadcrumbs do %>
  <%= render "/@shared/breadcrumbs/properties" %>
  <%= render "/@shared/breadcrumbs/add_property", class: "active" %>
<% end %>

<% content_for :page_heading do %>
  <%= page_heading "Adding", "Property",
    subtitle: "Fill out the form below",
    icon: "fas fa-browser" %>
<% end %>

<div class="bg-white p-4">
  <%= render "/properties/form", property: @property %>
</div>