<tr>
  <td class="text-left align-middle text-secondary"><%= @campaign.name %></td>
  <td class="text-center align-middle text-secondary"><%= number_with_delimiter @property.campaign_impressions_count(@campaign, @start_date, @end_date) %></td>
  <td class="text-center align-middle text-secondary"><%= number_with_delimiter @property.campaign_clicks_count(@campaign, @start_date, @end_date) %></td>
  <td class="text-center align-middle text-secondary"><%= number_to_percentage @property.campaign_click_rate(@campaign, @start_date, @end_date), precision: 2 %></td>
  <td class="text-center align-middle text-secondary"><%= @property.estimated_campaign_property_revenue(@campaign, @start_date, @end_date).format %></td>
</tr>
